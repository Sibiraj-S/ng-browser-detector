{
  "name": "ng-browser-detector",
  "version": "1.0.1",
  "description": "AngularJs service to detect platform, browser and version",
  "scripts": {
    "build": "grunt build",
    "cz": "git-cz",
    "develop": "grunt develop",
    "lint": "grunt lint",
    "postbuild": "sh ./scripts/postbuild.sh",
    "prebuild": "sh ./scripts/prebuild.sh",
    "precommit": "yarn run lint && yarn run test:single-run",
    "prerelease": "yarn run build",
    "release": "sh ./scripts/release.sh",
    "start": "grunt serve",
    "test": "./node_modules/.bin/karma start",
    "test:single-run": "./node_modules/.bin/karma start --browsers Firefox --single-run",
    "watch": "grunt watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sibiraj-S/ng-browser-detector.git"
  },
  "keywords": [
    "angularjs-browser-detector",
    "platform-detector",
    "ng-browser-detector",
    "browser-detector",
    "device-detector",
    "angularjs-device-detector"
  ],
  "author": "Sibiraj-S",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sibiraj-S/ng-browser-detector/issues"
  },
  "homepage": "https://github.com/Sibiraj-S/ng-browser-detector#readme",
  "peerDependencies": {
    "angular": ">=1.6.0"
  },
  "devDependencies": {
    "angular": "^1.6.7",
    "angular-mocks": "^1.6.7",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "grunt": "^1.0.1",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^3.2.1",
    "grunt-contrib-watch": "^1.0.0",
    "husky": "^0.14.3",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "load-grunt-tasks": "^3.5.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
